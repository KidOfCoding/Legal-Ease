{
  "expo": {
    "name": "LegalEase",
    "slug": "legalease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "legalease",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-image-picker",
      "expo-document-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "12b70542-320c-4863-ace9-38e1b78480ba"
      }
    },
    "android": {
      "package": "com.dannycharleesorganization.legalease",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    }
  }
}